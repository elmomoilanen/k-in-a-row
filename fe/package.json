        "@sveltejs/kit": "^2.5.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@types/node": "^22.0.2",
        "@typescript-eslint/eslint-plugin": "^7.14.1",
        "@typescript-eslint/parser": "^7.14.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.41.0",
        "prettier": "^3.3.2",
        "prettier-plugin-svelte": "^3.2.2",
        "svelte": "^4.2.9",
        "svelte-check": "^3.6.3",
        "tslib": "^2.6.2",
        "typescript": "^5.5.2",
        "vite": "^5.1.4",
        "vitest": "^1.4.0"
    },
    "type": "module"
